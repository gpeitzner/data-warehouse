                }
            }

        }

        private void LoadPurchase(string[] SourcePurchaseFiles, string DataDelimiter, string PurchaseTarget)
        {
            foreach (string sourcePurchaseFile in SourcePurchaseFiles)
            {
                SqlConnection sqlConnection = Dts.Connections["DataWarehouse"].AcquireConnection(Dts.Transaction) as SqlConnection;
                int lineCounter = 0;
                string line;
                StreamReader file = new StreamReader(sourcePurchaseFile);
                while ((line = file.ReadLine()) != null)
                {
                    if (lineCounter > 0)
                    {
                        string[] data = line.Split(DataDelimiter.ToCharArray()[0]);
                        foreach (string item in data)
                        {
                            Console.WriteLine(item);
                        }
                        string query = "INSERT INTO " + PurchaseTarget + "(purchase_date, supplier_code, supplier_name, supplier_address, supplier_number, supplier_web," +
                            "product_code, product_name, product_brand, product_category, branch_office_code, branch_office_name, branch_office_address, " +
                            "region, department, purchase_units, purchase_cost) VALUES('" + data[0] + "','" + data[1]
                            + "','" + data[2] + "','" + data[3] + "','" + data[4] + "','" + data[5] + "','" + data[6]
                            + "','" + data[7] + "','" + data[8] + "','" + data[9] + "','" + data[10] + "','" + data[11]
                            + "','" + data[12] + "','" + data[13] + "','" + data[14] + "','" + data[15] + "','" + data[16] + "')";
                        SqlCommand sqlCommand = new SqlCommand(query, sqlConnection);
                        sqlCommand.ExecuteNonQuery();
                    }
                    lineCounter++;
                }
                file.Close();
                Dts.TaskResult = (int)ScriptResults.Success;
            }
        }

        private void LoadSale(string[] SourceSaleFiles, string DataDelimiter, string SaleTarget)
        {
            foreach (string sourceSaleFile in SourceSaleFiles)
            {
                SqlConnection sqlConnection = Dts.Connections["DataWarehouse"].AcquireConnection(Dts.Transaction) as SqlConnection;
                int lineCounter = 0;
                string line;
                StreamReader file = new StreamReader(sourceSaleFile);
                while ((line = file.ReadLine()) != null)
                {
                    if (lineCounter > 0)
                    {
                        string[] data = line.Split(DataDelimiter.ToCharArray()[0]);
                        foreach (string item in data)
                        {
                            Console.WriteLine(item);
                        }
                        string query = "INSERT INTO " + SaleTarget + "(sale_date, client_code, client_name, client_type, client_address, client_number," +
                            "seller_code, seller_name, seller_vacationer, product_code, product_name, product_brand, product_category, " +
                            "branch_office_code, branch_office_name, branch_office_address, region, department, sale_units, sale_price) VALUES('" + data[0] + "','" + data[1]
