        public void Main()
        {
            // TODO: Add your code here
            try
            {
                string DataDirectory = Dts.Variables["User::DataDirectory"].Value.ToString();
                string DataDelimiter = Dts.Variables["User::DataDelimiter"].Value.ToString();
                string PurchaseTarget = Dts.Variables["User::DB1PurchaseTarget"].Value.ToString();
                string SaleTarget = Dts.Variables["User::DB1SaleTarget"].Value.ToString();
                string PurchaseFileExtension = Dts.Variables["User::PurchaseFileExtension"].Value.ToString();
                string SaleFileExtension = Dts.Variables["User::SaleFileExtension"].Value.ToString();
                string[] SourcePurchaseFiles = Directory.GetFiles(DataDirectory, "*" + PurchaseFileExtension);
                LoadPurchase(SourcePurchaseFiles, DataDelimiter, PurchaseTarget);
                string[] SourceSaleFiles = Directory.GetFiles(DataDirectory, "*" + SaleFileExtension);
                LoadSale(SourceSaleFiles, DataDelimiter, SaleTarget);
            }
            catch (Exception ex)
            {
                using (StreamWriter streamWriter = File.CreateText(Dts.Variables["User::ErrorDirectory"].Value.ToString() + "\\" + "ErrorLog.log"))
